

@model int
<a asp-action="Edit" asp-route-id="@Model" class="btn btn-success mr-1"><i class="fas fa-edit"></i></a>
<a asp-action="Details" asp-route-id="@Model" class="btn btn-info mr-1"><i class="fas fa-eye"></i></a>
<a asp-action="Delete" asp-route-id="@Model" class="btn btn-danger"><i class="fas fa-trash-alt"></i></a>